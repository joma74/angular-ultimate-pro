{
	"name": "angular-ultimate-pro",
	"version": "0.0.1",
	"description": "Replay after https://github.com/UltimateAngular/angular-fundamentals-src",
	"main": "main",
	"repository": "https://github.com/joma74/angular-ultimate-fundamentals.git",
	"author": "joma74 <dev-mails@gmx.net>",
	"license": "MIT",
	"private": false,
	"config": {
		"target": "target",
		"reports": "target/reports",
		"screenshots": "target/reports/screenshots/actual/",
		"screenshot-path-pattern": "${FIXTURE}/${TEST}/${USERAGENT}/${FILE_INDEX}.png"
	},
	"scripts": {
		"clean": "del 'target/*' '!.gitkeep'",
		"_lint": "tslint --project '.' 'src/**/*.ts' './config/**/*.js'",
		"lint": "yarn run _lint --fix",
		"tsc:validate": "node_modules/typescript/bin/tsc --project ./tsconfig.json --noEmit true; exitCode=$?; echo $exitCode; exit $exitCode",
		"tsc:trace": "node_modules/typescript/bin/tsc --project ./tsconfig.json --noEmit true --traceResolution true  | tee target/tsc.trace.log; echo $?",
		"tsc:listFiles": "node_modules/typescript/bin/tsc --project ./jsconfig.json --noEmit true --listFiles  | tee target/tsc.listFiles.log; echo $?",
		"start": "cross-env NODE_ENV=development webpack-dev-server --watch --hot --no-progress --config=webpack.config.js",
		"test:karma:once": "karma start karma.config.js",
		"test:karma:watch": "karma start karma.config.js --auto-watch --no-single-run",
		"test:servencafe:once": "start-server-and-test start http-get://localhost:4001/index.html test:cafe:once",
		"test:cafe:once": "testcafe 'firefox:headless,chrome:headless --no-sandbox --disable-gpu' -S -s $npm_package_config_screenshots -p $npm_package_config_screenshot_path_pattern -r spec,xunit:$npm_package_config_reports/report.xml src/tests/index-test.js",
		"ci": "npm-run-all _lint tsc:validate clean test:karma:once test:servencafe:once",
		"build": "rimraf dist && webpack --config config/webpack.prod.config.js --progress --profile --bail"
	},
	"resolutions": {
		"**/@types/webpack": "~3",
		"**/typescript": "~3"
	},
	"devDependencies": {
		"@types/core-js": "^2.5.0",
		"@types/extract-text-webpack-plugin": "^3.0",
		"@types/html-webpack-plugin": "3.2.0",
		"@types/jasmine": "^2.8.8",
		"@types/karma": "^1",
		"@types/webpack": "~4.4.12",
		"@types/webpack-dev-server": "^3.1.1",
		"@types/webpack-env": "^1.13.6",
		"@types/webpack-merge": "^4.1.3",
		"angular2-template-loader": "^0.6.2",
		"awesome-typescript-loader": "~5.2.1",
		"cross-env": "^5.2.0",
		"css-loader": "^1.0.0",
		"del-cli": "^1.1.0",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^2.0.0",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "~3",
		"jasmine-core": "^3.1.0",
		"json-server": "^0.14.0",
		"karma": "^3.0.0",
		"karma-chrome-launcher": "^2.2.0",
		"karma-jasmine": "^1.1.2",
		"karma-jasmine-html-reporter": "^1.1.0",
		"karma-sourcemap-loader": "^0.3.7",
		"karma-webpack": "~3",
		"node-sass": "^4.9.0",
		"npm-run-all": "^4.1.3",
		"null-loader": "^0.1.1",
		"postcss-loader": "^3.0.0",
		"prettier": "^1.13.4",
		"raw-loader": "^0.5.1",
		"rimraf": "^2.6.2",
		"sass-loader": "^7.0.3",
		"start-server-and-test": "^1.7.1",
		"style-loader": "^0.23.0",
		"tailwindcss": "^0.6.4",
		"testcafe": "^0.22.0",
		"testcafe-angular-selectors": "^0.3.0",
		"tslint": "^5.10.0",
		"tslint-config-prettier": "^1.13.0",
		"tslint-eslint-rules": "^5.3.1",
		"typescript": "~3",
		"webpack": "~4.20.2",
		"webpack-dev-server": "3.1.9",
		"webpack-merge": "~4.1.4"
	},
	"dependencies": {
		"@angular/common": "~6.1.9",
		"@angular/compiler": "~6.1.9",
		"@angular/core": "~6.1.9",
		"@angular/forms": "~6.1.9",
		"@angular/http": "~6.1.9",
		"@angular/platform-browser": "~6.1.9",
		"@angular/platform-browser-dynamic": "~6.1.9",
		"@angular/router": "~6.1.9",
		"core-js": "^2.5.7",
		"rxjs": "~6.3.3",
		"zone.js": "^0.8.26"
	},
	"engineStrict": true,
	"engines": {
		"node": ">= 8.9.4",
		"yarn": ">= 1.7.0"
	}
}
